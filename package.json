  "bugs": {
    "url": "https://github.com/DreamHi/HiCrawler/issues"
  },
  "homepage": "http://www.hicrawler.hidream.tech/",
  "dependencies": {
    "antd": "3.20.0",
    "axios": "0.19.0",
    "history": "4.9.0",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-beautiful-dnd": "11.0.4",
    "react-dom": "16.8.6",
    "react-intl": "2.9.0",
    "react-router-dom": "5.0.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "enzyme": "3.10.0",
