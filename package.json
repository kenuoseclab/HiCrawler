{
  "name": "HiCrawler",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --env=jsdom --collectCoverage",
    "precommit": "npm run lint-staged",
    "lint-staged": "lint-staged",
    "eslint": "./node_modules/.bin/eslint --ext .js,.jsx ./src/**",
    "prettier": "./node_modules/prettier/bin-prettier.js --write",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:DreamHi/HiCrawler.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DreamHi/HiCrawler/issues"
  },
  "homepage": "http://www.hicrawler.hidream.tech/",
  "dependencies": {
    "antd": "3.18.1",
    "axios": "0.18.0",
    "babel-polyfill": "6.26.0",
    "history": "4.7.2",
    "lodash": "4.17.10",
    "moment": "2.22.2",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-intl": "2.4.0",
    "react-router-dom": "4.3.1",
    "react-beautiful-dnd": "9.0.2",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "axios-mock-adapter": "1.15.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.2.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "gh-pages": "2.0.1",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "prettier": "1.13.7",
    "raf": "3.4.0",
    "react-scripts": "2.1.1",
    "react-styleguidist": "^8.0.5",
    "source-map-explorer": "^1.6.0",
    "stylelint": "9.4.0",
    "stylelint-config-prettier": "4.0.0",
    "stylelint-config-standard": "18.2.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup/setupTests.js",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/setup/setupFile.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/test/unit/**/*.test.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/test/setup/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/test/setup/fileTransform.js"
    },
    "coverageDirectory": "<rootDir>/test/coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "./src/**/*.{js,jsx,css}": [
        "npm run prettier",
        "git add"
      ],
      "./src/**/*.css": "stylelint --fix"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
